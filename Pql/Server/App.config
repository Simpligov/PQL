<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
        <section name="StructureMap" type="StructureMap.Configuration.StructureMapConfigurationSection, StructureMap"/>
        <section name="mycompanyHost" type="Pql.IntegrationStubs.WindowsServiceConfigSection, Pql.IntegrationStubs"/>
    </configSections>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
    </startup>
    <runtime>
        <gcServer enabled="true"/>
    </runtime>
    <mycompanyHost windowsServiceName="PqlServer-local" windowsServiceDisplayName="PqlServer - local" windowsServiceDescription="PqlServer. Installed for: local"/>
    <log4net>
        <appender name="RollingLogFileAppender_DEBUG" type="log4net.Appender.ColoredConsoleAppender">
            <threshold value="WARN"/>
            <layout type="log4net.Layout.PatternLayout">
                <conversionpattern value="%date{DATE} [%thread] %-5level %logger - %message%newline"/>
            </layout>
        </appender>
        <logger name="Pql">
            <level value="WARN"/>
            <appender-ref ref="RollingLogFileAppender_DEBUG"/>
        </logger>
    </log4net>
    <appSettings>
        <add key="storageDriverKey" value="ram"/>
        <add key="storageDriverInitString" value="c:\temp\ramdriver2"/>
        <add key="eqlProcessorInstanceName" value="default"/>
        <add key="eqlProcessorBaseAddress" value="net.tcp://localhost:5000/default"/>
    </appSettings>
</configuration>
